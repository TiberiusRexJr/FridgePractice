<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:d="http://xamarin.com/schemas/2014/forms/design"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d"
             x:Class="WPFNavigation_Practice.MainPage"
             xmlns:local="clr-namespace:WPFNavigation_Practice">

    <StackLayout>
        <!-- Place new controls here -->
        <Label Text="UserName: "></Label>
        <Entry Placeholder="Email" x:Name="EntryUserEmail" BindingContextChanged="EntryUserEmail_BindingContextChanged" />
        <Label Text="Password:"></Label>
        <Entry Placeholder="Password"></Entry>
        <Button Text="Login" Command="{Binding }"></Button>
    </StackLayout>
    
    

</ContentPage>
